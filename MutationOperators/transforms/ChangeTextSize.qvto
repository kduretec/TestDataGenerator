import m2m.qvt.oml.ExampleJavaLib;

modeltype PIM uses PIM('http://benchmarkdp.datagenerator.model.PIM');
	

transformation ChangeTextSize(inout model:PIM);

main() {
	var texts:= model.objectsOfType(Word); 
	texts->asSequence()->at(getRandom(texts->size()))->map ChangeSize()
	
}

mapping inout Word::ChangeSize() {
	size:= getRandom(50)
}