import m2m.qvt.oml.ExampleJavaLib;

modeltype PSM uses PSMDoc('http://benchmarkdp.datagenerator.model.PSMDoc');
	

transformation ChangeTextSize(inout model:PSM);

main() {
	var texts:= model.objectsOfType(SimpleText); 
	if (texts->size() > 0) {
		texts->asSequence()->at(getRandom(texts->size()))->map ChangeSize()	
	}
	
	
}

mapping inout SimpleText::ChangeSize() {
	size:= getRandomBetween(4,20);
}