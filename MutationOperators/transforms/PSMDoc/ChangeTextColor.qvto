import m2m.qvt.oml.ExampleJavaLib;

modeltype PSM uses PSMDoc('http://benchmarkdp.datagenerator.model.PSMDoc');

	
transformation ChangeTextColor(inout model:PSM);

main() {
	var texts:= model.objectsOfType(SimpleText); 
	texts->asSequence()->at(getRandom(texts->size()))->map ChangeColor()
	
}

mapping inout SimpleText::ChangeColor() { 
	color:=Color::RED;
}	